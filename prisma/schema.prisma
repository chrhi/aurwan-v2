generator client {
  provider = "prisma-client-js"
   
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Account {
  id                 String            @unique @default(cuid())
  userId             String            @unique
  
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Products {
  id                 String            @unique @default(cuid())
  userId             String            @unique
  title              String 
  overview           String 
  description        Json
  price              Decimal
  compareAtPrice     Decimal
  media              Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt          
}